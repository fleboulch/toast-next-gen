<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity EntreprisePartenaire.
    -->
    <changeSet id="20170125144912-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="entreprise_id"
                                 baseTableName="entreprise_partenaire"
                                 constraintName="fk_entreprise_partenaire_entreprise_id"
                                 referencedColumnNames="id"
                                 referencedTableName="entreprise"/>

        <addForeignKeyConstraint baseColumnNames="entreprise_partenaires_id"
                                 baseTableName="entreprise_partenaire_diplome"
                                 constraintName="fk_entreprise_partenaire_diplome_entreprise_partenaires_id"
                                 referencedColumnNames="id"
                                 referencedTableName="entreprise_partenaire"/>
        <addForeignKeyConstraint baseColumnNames="diplomes_id"
                                 baseTableName="entreprise_partenaire_diplome"
                                 constraintName="fk_entreprise_partenaire_diplome_diplomes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="diplome"/>
        
    </changeSet>
</databaseChangeLog>
